<div>
    <h2>Lista de productos</h2>
    <div id="products">
        <ul>
            {{#each prods}}
            <li>Titulo:{{title}},
                Id:{{id}},
                Precio:{{precio}},
                Stock:{{stock}}
            </li>
            {{/each}}
        </ul>
    </div>
    <a href="http://localhost:8080/realTimeProducts">
        <h2>Agregar Productos</h2>
        <h2>Productos Actualizados en tiempo real</h2>
    </a>
</div>